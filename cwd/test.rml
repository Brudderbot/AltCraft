            redLvl = 0
            function Test(elem)
                elem.style["background-color"] = "rgb(" .. redLvl .. ",255,255)"
                print("New background-color: "..elem.style["background-color"])
                redLvl = redLvl + 50
            end
        </script>
    </head>
    <body>
        Test RmlUi
